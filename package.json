    "license": "MIT",
    "bugs": {
        "url": "https://github.com/mmierzwa/js-dev-env/issues"
    },
    "homepage": "https://github.com/mmierzwa/js-dev-env#readme",
    "dependencies": {
        "whatwg-fetch": "1.0.0"
    },
    "devDependencies": {
